{
  "expo": {
    "name": "better-aid",
    "jsEngine": "hermes",
    "slug": "better-aid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AppIconBigger.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/App.png",
      "resizeMode": "contain"
    },
    "notification": {
      "icon": "./assets/images/AppIconBigger.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.shoppingList"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "49b30fef-6038-4028-9a49-a1db0f955601"
      }
    }
  }
}
